{
  "name": "obsidian-scroll-control",
  "version": "1.0.0",
  "description": "An Obsidian plugin that adds configurable scroll control buttons for easy navigation",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write ."
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "scroll",
    "navigation"
  ],
  "author": "zxai",
  "authorUrl": "https://github.com/zxai-io",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "builtin-modules": "3.3.0",
    "esbuild": "0.25.3",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.4.0",
    "obsidian": "latest",
    "prettier": "^3.3.2",
    "tslib": "2.8.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.31.1"
  }
}
